<!-- Setting Finance Menu -->
<div class="panel panel-default">
    <div class="panel-head">
        <div class="panel-title">
            <i class="icon-settings panel-head-icon"></i>
            <span class="panel-title-text">{{ lang.text_email_template_menu }}</span>
        </div>
    </div>
    <div class="panel-wrapper">
        <div class="nav flex-column vnav-tabs">
            {% for value in template_menu %}
                <li id="{{ value.template }}" class="nav-link"><a href="{{ common.url_route }}emailtemplate&for={{ value.template }}">{{ value.name }}</a></li>
            {% endfor %}
        </div>
    </div>
</div>
